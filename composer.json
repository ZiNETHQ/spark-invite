{
    "name": "zinethq/spark-invite",
    "description": "User invite system for Laravel Spark",
    "keywords": ["team", "user", "laravel", "spark", "invite", "zinethq"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Nicholson",
            "email": "jon.nicholson@zinethq.com",
            "homepage": "http://www.zinethq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "laravel/spark": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "ZiNETHQ\\SparkInvite\\": "src/"
        }
    },
        "repositories": [
        {
            "type": "composer",
            "url": "https://spark-satis.laravel.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
